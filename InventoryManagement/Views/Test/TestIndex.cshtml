<input id="automplete">



<script>

    $(function () {
        $("#automplete").autocomplete({
            source: function (req, res) {
                $.ajax({
                    url: "/Form/AutoComplete",
                    dataType: "json",
                    success: function (data) {
                        console.log(data)
                        res(data);
                    }
                })
            }
        });
    });

</script>